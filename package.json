{
  "private": true,
  "scripts": {
    "dev": "turbo run dev --filter wfo-ui",
    "dev:all": "turbo run dev",
    "build": "turbo run build",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install",
    "prettier": "prettier -c \"{apps/**/*.{ts,tsx,json,md},libs/**/*{ts,tsx,json,md},*.{ts,js,json,md}}\"",
    "prettier-fix": "prettier --write \"{apps/**/*.{ts,tsx,json,md},libs/**/*{ts,tsx,json,md},*.{ts,js,json,md}}\"",
    "lint": "turbo run lint",
    "tsc": "turbo run tsc"
  },
  "devDependencies": {
    "@turbo/gen": "^1.9.7",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "prettier": "^2.5.1",
    "turbo": "latest",
    "husky": "^8.0.0"
  },
  "name": "orchestrator-ui-core",
  "packageManager": "yarn@1.22.19",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
